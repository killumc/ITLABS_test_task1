                    <Run Text="{StaticResource PrivacyPolicy_1_7}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_1_8}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_1_8_1}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_1_8_2}" /> <LineBreak />
                    <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Title}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_1}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref1}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref2}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref3}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref4}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref5}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref6}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref7}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref8}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref9}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref10}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref11}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref12}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_2_Ref13}" /> <LineBreak />
                    <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_Title}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_1}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_1_1}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_1_2}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_1_3}" /> <LineBreak />
