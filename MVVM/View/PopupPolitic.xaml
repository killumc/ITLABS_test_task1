                Width="338"
                Height="94"
                Margin="0,80,0,0"
                Command="{Binding PoliticPopup_CloseCommand}"
                Content="{StaticResource Exit}"
                FontSize="24"
                Style="{StaticResource popup_btn}" />
        </StackPanel>
