                    <Run Text="{StaticResource PrivacyPolicy_3_2_5}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_3}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_4}" /> <LineBreak />
                    <Run Text="{StaticResource PrivacyPolicy_3_5}" /> <LineBreak />
